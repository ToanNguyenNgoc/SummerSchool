{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "title": {
      "type": "string"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "teacher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::teacher.teacher",
      "inversedBy": "courses"
    }
  }
}
